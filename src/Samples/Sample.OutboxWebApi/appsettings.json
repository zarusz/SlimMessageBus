{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "SlimMessageBus": "Information",
      "SlimMessageBus.Host.Outbox": "Trace",
      "Microsoft": "Warning"
    }
  },
  "Kafka": {
    "Brokers": "{{kafka_brokers}}",
    "Username": "{{kafka_username}}",
    "Password": "{{kafka_password}}",
    "Secure": "{{kafka_secure}}"
  },
  "Azure": {
    "ServiceBus": "{{azure_servicebus_connectionstring}}"
  },
  "ConnectionStrings": {
    "SqlServerConnection": "{{sqlserver_connectionstring}}",
    "PostgreSqlConnection": "{{postgres_connectionstring}}"
  }
}
